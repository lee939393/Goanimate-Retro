	function createNewChar(themeId) {
		if (typeof pageTracker != 'undefined' && pageTracker)
			pageTracker._trackPageview("/pageTracker/ccbrowser/close");
		window.location = '/go/charactercreator/new_char/?themeId=' + themeId + '';
	}

	function showOG() {
		showOverlay(jQuery('#overlayGeneral'));
